{
    "experiment": "GraphMerge",
    "group":  "sweep_smass", 
    "move":false,
    "model":  "Sage",
    "log": true,

    "run_params":{
    "n_epochs":    250,
    "n_trials": 1,
    "batch_size": 512,
    "val_epoch":   2,
    "early_stopping": true,
    "patience": 20,
    "l1_lambda":0,
    "l2_lambda":1e-5,
    "loss_func": "Gauss1d",
    "metrics": "test_multi",
    "performance_plot": "SAM_base",
    "shuffle":true,
    "save":true,
    "seed":false,
    "num_workers": 4
}, 
"learn_params":{
    "learning_rate":   3e-3,
    "schedule":   "warmup_expcos",
    "g_up":1.3,
    "g_down":0.95,
    "warmup":4,
    "period":10, 
    "eta_min":1e-5
},

    "hyper_params": {
        "hidden_channels":   64,  
        "conv_layers":   3,
        "conv_activation": "relu",
        "decode_activation": "leakyrelu",
        "decode_layers":   3,
        "layernorm":      true,
        "agg": "sum",
        "variance":     true,
        "rho":        0
    },

   "data_params":{ 
    "case": "vlarge_all_4t_z0.0_quantile_raw",
    "targets": [0],
    "split": 0.8,
    "shuffle": 0
}
}