{
    "experiment": "GraphMerge",
    "group":  "sweep_others", 
    "move":false,
    "model":  "Sage",
    "log": true,

    "run_params":{
    "n_epochs":    200,
    "n_trials": 2,
    "batch_size": 128,
    "val_epoch":   2,
    "early_stopping": true,
    "patience": 100,
    "l1_lambda":0.0,
    "l2_lambda":0.0,
    "loss_func": "Gauss1d",
    "metrics": "test_multi",
    "performance_plot": "multi_base",
    "shuffle":true,
    "save":true,
    "seed":false,
    "num_workers": 4
}, 
"learn_params":{
    "learning_rate":   1e-2,
    "schedule":   "onecycle",
    "g_up":1.3,
    "g_down":0.98,
    "warmup":5,
    "period":10, 
    "eta_min":1e-5
},

    "hyper_params": {
        "hidden_channels":   128,  
        "conv_layers":   1,
        "conv_activation": "relu",
        "decode_activation": "leakyrelu",
        "decode_layers":   3,
        "layernorm":      true,
        "agg": "sum",
        "variance":     true,
        "rho":        0
    },

   "data_params":{ 
    "case": "vlarge_all_4t_quantile_raw_final",
    "targets": [0],
    "split": 0.8,
    "shuffle": 0,
    "del_feats": [],
    "scale": 0
}
}